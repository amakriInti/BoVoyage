@model List<BoVoyage.Scenario1.Dal.Dossier>
@using BoVoyage.Scenario1.Models;
@{
    ViewBag.Title = "Dossier";
}

<h2>Dossier</h2>

<body>
    <ul>
        @{
            //<li>@Model.Count()</li>;
            foreach (var v in Model)
            {
                var etat = (DossierEnum)v.Etat;
                if (v.Commercial != null)
                {
                    <li>
                        <hr />
                        @v.Client1.Nom | @v.Voyage1.Libelle | @etat  |  @v.Employe.Login  | @Html.ActionLink("Affecter/Changer Commerical", "AffecterCommercial", new { id = v.Id }) | @Html.ActionLink("Valider Dossier", "ValidDossier", new { id = v.Id })
                    </li>
                }
                else
                {
                    <li>
                        <hr />
                        @v.Client1.Nom | @v.Voyage1.Libelle | @etat  | @Html.ActionLink("Affecter/Changer Commerical", "AffecterCommercial", new { id = v.Id })
                    </li>
                }


            }
        }
    </ul>
    <br />
    <p>
        @Html.ActionLink("Back to Menu", "Index2")
    </p>
</body>
</html>
